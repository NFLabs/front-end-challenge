{
  "name": "slack-clone",
  "version": "0.0.1",
  "description": "A simplified front-end only slack clone.",
  "main": "app/index.js",
  "scripts": {
    "bundle": "./node_modules/.bin/webpack",
    "dev-server": "./node_modules/.bin/webpack-dev-server",
    "lint": "./node_modules/.bin/eslint .",
    "build-database": "./node_modules/.bin/babel-node ./mock-database/main.js",
    "start": "npm run dev-server",
    "test": "./node_modules/.bin/babel-node ./node_modules/.bin/_mocha --require ./test/setup.js 'test/**/*.js'",
    "test:watch": "npm run test -- --watch --watch-extensions js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NFLabs/front-end-challenge.git"
  },
  "author": "Bertrand Boustany",
  "bugs": {
    "url": "https://github.com/NFLabs/front-end-challenge/issues"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "homepage": "https://github.com/NFLabs/front-end-challenge#readme",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-resolver": "0.0.7",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.0",
    "chance": "^1.0.3",
    "enzyme": "^2.4.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-import-resolver-webpack": "^0.3.2",
    "eslint-loader": "^1.4.0",
    "eslint-plugin-import": "^1.10.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "immutable": "^3.8.1",
    "jsdom": "^9.4.1",
    "json-loader": "^0.5.4",
    "lodash": "^4.13.1",
    "mocha": "^2.5.3",
    "react": "^15.2.0",
    "react-addons-test-utils": "^15.2.0",
    "react-dom": "^15.2.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "redux": "^3.5.2",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
